{
  "name": "@nori-dot-com/project",
  "version": "1.2.0",
  "description": "Nori supplier project specification and utilities",
  "keywords": [
    "nori"
  ],
  "author": "jaycenhorton <jaycen@nori.com>",
  "homepage": "https://nori.com",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nori-dot-eco/nori-dot-com.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "make:docs": "yarn run typedoc --readme none --stripInternal --excludeExternals --plugin typedoc-plugin-markdown  src/specification.ts src/validation.ts src/utils/convertFromV2ToV1.ts",
    "make:schema": "yarn run typescript-json-schema src/specification.ts Project -o ./src/json/specification.json --strictNullChecks=true --required=true --titles=true --topRef=true --noExtraProps=true --defaultProps=true --validationKeywords=\"errorMessage\"",
    "build": "yarn run make:schema && tsc",
    "prepare": "yarn run build",
    "watch": "tsc -w -p ."
  },
  "bugs": {
    "url": "https://github.com/nori-dot-eco/nori-dot-com/issues"
  },
  "devDependencies": {
    "@types/ajv-errors": "^1.0.2",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.15",
    "typedoc": "^0.18.0",
    "typedoc-plugin-markdown": "^2.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "ajv-errors": "^1.0.1",
    "typescript-json-schema": "^0.42.0"
  }
}
