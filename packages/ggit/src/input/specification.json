{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "definitions": {
        "BurnEvent": {
            "additionalProperties": false,
            "description": "Burn event.",
            "properties": {
                "BurnTime": {
                    "$ref": "#/definitions/BurnTime",
                    "title": "BurnTime"
                }
            },
            "required": [
                "BurnTime"
            ],
            "title": "BurnEvent",
            "type": "object"
        },
        "BurnTime": {
            "description": "Time burn event occurred.",
            "enum": [
                "no burning",
                "yes, after harvesting",
                "yes, before planting"
            ],
            "title": "BurnTime",
            "type": "string"
        },
        "CRP": {
            "description": "CRP.",
            "enum": [
                "no",
                "yes"
            ],
            "title": "CRP",
            "type": "string"
        },
        "CRPType": {
            "description": "CRP Type. Dependant on CRP.",
            "enum": [
                "100% grass",
                "grass/legume mixture",
                "none"
            ],
            "title": "CRPType",
            "type": "string"
        },
        "ContinuedCrop": {
            "additionalProperties": false,
            "properties": {
                "@CropNumber": {
                    "$ref": "#/definitions/CropNumber",
                    "title": "@CropNumber"
                },
                "BurnEvent": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/BurnEvent"
                        }
                    ],
                    "default": {},
                    "title": "BurnEvent"
                },
                "ContinueFromPreviousYear": {
                    "const": "y",
                    "description": "Was this crop planted in the previous year.\nYes = continue crop growth for non-woody perennial crops\n(like alfalfa, perennial grass hay or pasture) from\nprevious year without re-planting.",
                    "title": "ContinueFromPreviousYear",
                    "type": "string"
                },
                "CropName": {
                    "$ref": "#/definitions/CropName",
                    "title": "CropName"
                },
                "GrazingList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/GrazingList"
                        }
                    ],
                    "default": {},
                    "title": "GrazingList"
                },
                "HarvestList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/HarvestList"
                        }
                    ],
                    "default": {},
                    "title": "HarvestList"
                },
                "IrrigationList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/IrrigationList"
                        }
                    ],
                    "default": {},
                    "title": "IrrigationList"
                },
                "LimingEvent": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/LimingEvent"
                        }
                    ],
                    "default": {},
                    "title": "LimingEvent"
                },
                "NApplicationList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/NApplicationList"
                        }
                    ],
                    "default": {},
                    "title": "NApplicationList"
                },
                "OMADApplicationList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/OMADApplicationList"
                        }
                    ],
                    "default": {},
                    "title": "OMADApplicationList"
                },
                "Prune": {
                    "description": "Prune. Yes = woody perennial crops are pruned.",
                    "enum": [
                        "no",
                        "yes"
                    ],
                    "title": "Prune",
                    "type": "string"
                },
                "Renew": {
                    "description": "Renew. Yes = remove and replant orchard. All coarse and fine branches\nand all coarse roots are removed, soil is plowed, and new saplings\nassumed to be 2 yrs old are planted.",
                    "enum": [
                        "no",
                        "yes"
                    ],
                    "title": "Renew",
                    "type": "string"
                },
                "TillageList": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/EmptyObject"
                        },
                        {
                            "$ref": "#/definitions/TillageList"
                        }
                    ],
                    "default": {},
                    "title": "TillageList"
                }
            },
            "required": [
                "@CropNumber",
                "BurnEvent",
                "ContinueFromPreviousYear",
                "CropName",
                "GrazingList",
                "HarvestList",
                "IrrigationList",
                "LimingEvent",
                "NApplicationList",
                "OMADApplicationList",
                "TillageList"
            ],
            "title": "ContinuedCrop",
            "type": "object"
        },
        "CropName": {
            "description": "Name of crop. Check GGIT_API_File_Specification.xlsx tab CropName for complete list",
            "enum": [
                "alfalfa",
                "almond",
                "annual rye",
                "annual rye - legume",
                "annual rye - legume - radish",
                "austrian winter pea",
                "avocados",
                "barley",
                "broccoli-coast",
                "broccoli-desert",
                "carrots",
                "cauliflower",
                "cereal rye",
                "cherries",
                "clover",
                "corn",
                "corn silage",
                "cotton",
                "dry field beans",
                "dry field pea",
                "english walnuts",
                "fallow",
                "forage radish",
                "grape, raisin",
                "grape, table",
                "grape, wine (1391-1670 gdd)",
                "grape, wine (1671-1950 gdd)",
                "grape, wine (<1390 gdd)",
                "grape, wine (>1950 gdd)",
                "grapefruit",
                "grass",
                "grass-legume mix",
                "lemons & limes",
                "lettuce-head",
                "lettuce-leaf",
                "lettuce-romaine",
                "millet",
                "oats",
                "oilseed radish",
                "olives",
                "oranges",
                "peaches and nectarines",
                "peanut",
                "pistachios",
                "potato",
                "rice - flooded",
                "rye",
                "sorghum",
                "sorghum silage",
                "soybean",
                "spring wheat",
                "strawberry",
                "sugar beets",
                "sunflower",
                "switchgrass",
                "tangerines & mandarins",
                "tomatoes, fresh",
                "tomatoes, processing",
                "vetch",
                "winter grain-other",
                "winter wheat"
            ],
            "title": "CropName",
            "type": "string"
        },
        "CropNumber": {
            "description": "Crop number for this crop event. Can have up to 3 crops for one crop year",
            "enum": [
                1,
                2,
                3
            ],
            "title": "CropNumber",
            "type": "number"
        },
        "CropScenario": {
            "additionalProperties": false,
            "description": "Cropland management scenario.",
            "properties": {
                "@Name": {
                    "$ref": "#/definitions/CropScenarioName",
                    "title": "@Name"
                },
                "CropYear": {
                    "items": {
                        "$ref": "#/definitions/CropYear<ContinuedCrop|NewCrop>"
                    },
                    "title": "CropYear",
                    "type": "array"
                }
            },
            "required": [
                "@Name",
                "CropYear"
            ],
            "title": "CropScenario",
            "type": "object"
        },
        "CropScenarioName": {
            "description": "Name of this crop scenario.",
            "enum": [
                "Current",
                "Future"
            ],
            "title": "CropScenarioName",
            "type": "string"
        },
        "CropYear<ContinuedCrop|NewCrop>": {
            "additionalProperties": false,
            "description": "Crop event year.",
            "properties": {
                "@Year": {
                    "description": "Year for this crop event.",
                    "title": "@Year",
                    "type": "number"
                },
                "Crop": {
                    "items": {
                        "anyOf": [
                            {
                                "$ref": "#/definitions/ContinuedCrop"
                            },
                            {
                                "$ref": "#/definitions/NewCrop"
                            }
                        ]
                    },
                    "title": "Crop",
                    "type": "array"
                }
            },
            "required": [
                "@Year",
                "Crop"
            ],
            "title": "CropYear<ContinuedCrop|NewCrop>",
            "type": "object"
        },
        "Cropland": {
            "additionalProperties": false,
            "description": "Cropland data",
            "properties": {
                "@Name": {
                    "description": "Name of this field.",
                    "title": "@Name",
                    "type": "string"
                },
                "CRP": {
                    "$ref": "#/definitions/CRP",
                    "title": "CRP"
                },
                "CRPEndYear": {
                    "description": "CRP end year. Must be YYYY.",
                    "items": [
                        {
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "CRPEndYear",
                    "type": "array"
                },
                "CRPStartYear": {
                    "description": "CRP start year. Must be YYYY.",
                    "items": [
                        {
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "CRPStartYear",
                    "type": "array"
                },
                "CRPType": {
                    "$ref": "#/definitions/CRPType",
                    "title": "CRPType"
                },
                "CropScenario": {
                    "items": {
                        "$ref": "#/definitions/CropScenario"
                    },
                    "title": "CropScenario",
                    "type": "array"
                },
                "GEOM": {
                    "$ref": "#/definitions/GEOM",
                    "title": "GEOM"
                },
                "PostCRPManagement": {
                    "description": "Post CRP management.",
                    "items": [
                        {
                            "enum": [
                                "irrigated: annual crops in rotation",
                                "irrigated: annual crops with hay/pasture in rotation",
                                "irrigated: continuous hay",
                                "irrigated: orchard or vineyard",
                                "non-irrigated: annual crops in rotation",
                                "non-irrigated: annual crops with hay/pasture in rotation",
                                "non-irrigated: continuous hay",
                                "non-irrigated: fallow-grain",
                                "non-irrigated: livestock grazing",
                                "non-irrigated: orchard or vineyard"
                            ],
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "PostCRPManagement",
                    "type": "array"
                },
                "PostCRPTillage": {
                    "description": "Post-CRP tillage.",
                    "items": [
                        {
                            "enum": [
                                "intensive tillage",
                                "no till",
                                "reduced tillage"
                            ],
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "PostCRPTillage",
                    "type": "array"
                },
                "Pre-1980": {
                    "$ref": "#/definitions/Pre1980",
                    "title": "Pre-1980"
                },
                "PreCRPManagement": {
                    "description": "Pre-CRP management.",
                    "items": [
                        {
                            "enum": [
                                "irrigated: annual crops in rotation",
                                "irrigated: annual crops with hay/pasture in rotation",
                                "irrigated: continuous hay",
                                "irrigated: orchard or vineyard",
                                "non-irrigated: annual crops in rotation",
                                "non-irrigated: annual crops with hay/pasture in rotation",
                                "non-irrigated: continuous hay",
                                "non-irrigated: fallow-grain",
                                "non-irrigated: livestock grazing",
                                "non-irrigated: orchard or vineyard"
                            ],
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "PreCRPManagement",
                    "type": "array"
                },
                "PreCRPTillage": {
                    "description": "Pre-CRP tillage.",
                    "items": [
                        {
                            "enum": [
                                "intensive tillage",
                                "no till",
                                "reduced tillage"
                            ],
                            "type": "string"
                        }
                    ],
                    "maxItems": 1,
                    "minItems": 0,
                    "title": "PreCRPTillage",
                    "type": "array"
                },
                "Year1980-2000": {
                    "description": "Historic management between 1980 - 2000. Check GGIT_API_File_Specification.xlsx for complete list.",
                    "enum": [
                        "irrigated: annual crops in rotation",
                        "irrigated: annual crops with hay/pasture in rotation",
                        "irrigated: continuous hay",
                        "irrigated: orchard or vineyard",
                        "non-irrigated: annual crops in rotation",
                        "non-irrigated: annual crops with hay/pasture in rotation",
                        "non-irrigated: continuous hay",
                        "non-irrigated: fallow-grain",
                        "non-irrigated: livestock grazing",
                        "non-irrigated: orchard or vineyard"
                    ],
                    "title": "Year1980-2000",
                    "type": "string"
                },
                "Year1980-2000_Tillage": {
                    "description": "Tillage from 1980 - 2000.",
                    "enum": [
                        "intensive tillage",
                        "no till",
                        "reduced tillage"
                    ],
                    "title": "Year1980-2000_Tillage",
                    "type": "string"
                }
            },
            "required": [
                "@Name",
                "CRP",
                "CRPEndYear",
                "CRPStartYear",
                "CRPType",
                "CropScenario",
                "GEOM",
                "PostCRPManagement",
                "PostCRPTillage",
                "Pre-1980",
                "PreCRPManagement",
                "PreCRPTillage"
            ],
            "title": "Cropland",
            "type": "object"
        },
        "EEP": {
            "description": "Enhanced efficiency product.",
            "enum": [
                "nitrification inhibitor",
                "none",
                "slow release"
            ],
            "title": "EEP",
            "type": "string"
        },
        "EmptyObject": {
            "additionalProperties": false,
            "title": "EmptyObject",
            "type": "object"
        },
        "GEOM": {
            "additionalProperties": false,
            "description": "Geometry, point or polygon. Polygon has maximum about of vertices, 1000.  Max size is 10,000 acres. SRID any",
            "properties": {
                "#text": {
                    "$ref": "#/definitions/WKT",
                    "title": "#text"
                },
                "@AREA": {
                    "description": "Area of this geometry in acres.",
                    "title": "@AREA",
                    "type": "number"
                },
                "@SRID": {
                    "const": "4326",
                    "description": "Standard spatial reference identifier.",
                    "title": "@SRID",
                    "type": "string"
                }
            },
            "required": [
                "#text",
                "@AREA",
                "@SRID"
            ],
            "title": "GEOM",
            "type": "object"
        },
        "Grain": {
            "description": "Is this a grain harvest",
            "enum": [
                "no",
                "yes"
            ],
            "title": "Grain",
            "type": "string"
        },
        "GrazingEvent": {
            "additionalProperties": false,
            "description": "Grazing event",
            "properties": {
                "GrazingEndDate": {
                    "description": "Grazing End date. Any date of the year. The system is sensitive to leap dates. Must be after the Grazing Start Date.",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "GrazingEndDate",
                    "type": "string"
                },
                "GrazingStartDate": {
                    "description": "Grazing Start date. Any date of the year. The system is sensitive to leap years",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "GrazingStartDate",
                    "type": "string"
                },
                "RestPeriod": {
                    "description": "Rest Period. Any number of days less than 365.",
                    "title": "RestPeriod",
                    "type": "number"
                },
                "UtilizationPct": {
                    "description": "Utilization Percentage. Any percentage, in whole or real numbers, between 0 and 100.",
                    "title": "UtilizationPct",
                    "type": "number"
                }
            },
            "required": [
                "GrazingEndDate",
                "GrazingStartDate",
                "RestPeriod",
                "UtilizationPct"
            ],
            "title": "GrazingEvent",
            "type": "object"
        },
        "GrazingList": {
            "additionalProperties": false,
            "description": "List of grazing events.",
            "properties": {
                "GrazingEvent": {
                    "items": {
                        "$ref": "#/definitions/GrazingEvent"
                    },
                    "title": "GrazingEvent",
                    "type": "array"
                }
            },
            "required": [
                "GrazingEvent"
            ],
            "title": "GrazingList",
            "type": "object"
        },
        "HarvestEvent": {
            "additionalProperties": false,
            "description": "Harvest event",
            "properties": {
                "Grain": {
                    "$ref": "#/definitions/Grain",
                    "title": "Grain"
                },
                "HarvestDate": {
                    "description": "Harvest Date. must be in MM/DD/YYYY",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "HarvestDate",
                    "type": "string"
                },
                "StrawStoverHayRemoval": {
                    "description": "% of straw/stover/have/residue removal.",
                    "title": "StrawStoverHayRemoval",
                    "type": "number"
                },
                "yield": {
                    "description": "Yield amount. See crop type spec for yield units in GGIT_API_File_Specification.xlsx.",
                    "title": "yield",
                    "type": "number"
                }
            },
            "required": [
                "Grain",
                "HarvestDate",
                "StrawStoverHayRemoval",
                "yield"
            ],
            "title": "HarvestEvent",
            "type": "object"
        },
        "HarvestList": {
            "additionalProperties": false,
            "description": "List of harvest events.",
            "properties": {
                "HarvestEvent": {
                    "items": {
                        "$ref": "#/definitions/HarvestEvent"
                    },
                    "title": "HarvestEvent",
                    "type": "array"
                }
            },
            "required": [
                "HarvestEvent"
            ],
            "title": "HarvestList",
            "type": "object"
        },
        "IrrigationEvent": {
            "additionalProperties": false,
            "description": "Irrigation Event",
            "properties": {
                "IrrigationDate": {
                    "description": "Irrigation date. must be mm/dd/yyyy",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "IrrigationDate",
                    "type": "string"
                },
                "IrrigationInches": {
                    "description": "Irrigation Inches",
                    "title": "IrrigationInches",
                    "type": "number"
                }
            },
            "required": [
                "IrrigationDate",
                "IrrigationInches"
            ],
            "title": "IrrigationEvent",
            "type": "object"
        },
        "IrrigationList": {
            "additionalProperties": false,
            "description": "List of irrigation events.",
            "properties": {
                "IrrigationEvent": {
                    "items": {
                        "$ref": "#/definitions/IrrigationEvent"
                    },
                    "title": "IrrigationEvent",
                    "type": "array"
                }
            },
            "required": [
                "IrrigationEvent"
            ],
            "title": "IrrigationList",
            "type": "object"
        },
        "LimingEvent": {
            "additionalProperties": false,
            "description": "Liming event.",
            "properties": {
                "LimingDate": {
                    "description": "Liming date. must be in mm/dd/yyyy format",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "LimingDate",
                    "type": "string"
                },
                "LimingMethod": {
                    "$ref": "#/definitions/LimingMethod",
                    "title": "LimingMethod"
                },
                "LimingRate": {
                    "description": "Liming Rate. units in tons/acre",
                    "title": "LimingRate",
                    "type": "number"
                }
            },
            "required": [
                "LimingDate",
                "LimingMethod",
                "LimingRate"
            ],
            "title": "LimingEvent",
            "type": "object"
        },
        "LimingMethod": {
            "description": "Liming Method",
            "enum": [
                "calcitic limestone",
                "crushed limestone",
                "dolomitic limestone",
                "none"
            ],
            "title": "LimingMethod",
            "type": "string"
        },
        "NApplicationEvent": {
            "additionalProperties": false,
            "description": "Nitrogen application event.",
            "properties": {
                "EEP": {
                    "$ref": "#/definitions/EEP",
                    "title": "EEP"
                },
                "NApplicationAmount": {
                    "description": "Nitrogen application amount. Units in lbs N/acre",
                    "title": "NApplicationAmount",
                    "type": "number"
                },
                "NApplicationDate": {
                    "description": "\"Nitrogen application date. must be in MM/DD/YYYY",
                    "pattern": "^02\\/(?:[01]\\d|2\\d)\\/(?:20)(?:0[048]|[13579][26]|[2468][048])|(?:0[13578]|10|12)\\/(?:[0-2]\\d|3[01])\\/(?:20)\\d{2}|(?:0[469]|11)\\/(?:[0-2]\\d|30)\\/(?:20)\\d{2}|02\\/(?:[0-1]\\d|2[0-8])\\/(?:20)\\d{2}$",
                    "title": "NApplicationDate",
                    "type": "string"
                },
                "NApplicationMethod": {
                    "$ref": "#/definitions/NApplicationMethod",
                    "title": "NApplicationMethod"
                },
                "NApplicationType": {
                    "$ref": "#/definitions/NApplicationType",
                    "title": "NApplicationType"
                }
            },
            "required": [
                "EEP",
                "NApplicationAmount",
                "NApplicationDate",
                "NApplicationMethod",
                "NApplicationType"
            ],
            "title": "NApplicationEvent",
            "type": "object"
        },
        "NApplicationList": {
            "additionalProperties": false,
            "description": "List of nitrogen applications.",
            "properties": {
                "NApplicationEvent": {
                    "items": {
                        "$ref": "#/definitions/NApplicationEvent"
                    },
                    "title": "NApplicationEvent",
                    "type": "array"
                }
            },
            "required": [
                "NApplicationEvent"
            ],
            "title": "NApplicationList",
            "type": "object"
        },
        "NApplicationMethod": {
            "description": "Nitrogen application method.",
            "enum": [
                "aerial application",
                "fertigation",
                "incorporate / inject",
                "surface band / sidedress",
                "surface broadcast"
            ],
            "title": "NApplicationMethod",
            "type": "string"
        },
        "NApplicationType": {
            "description": "Nitrogen application type.",
            "enum": [
                "ammonium nitrate (34-0-0)",
                "ammonium nitrate phosphate (23-23-00)",
